@model List<Festispec.WebApplication.Models.Beschikbaarheid_inspecteurs>


@section Scripts{
    <script src="~/Scripts/Calendar/baseCalendar.js" type="text/javascript"></script>
    <script></script>
}

@section Style{
    <link href="~/Content/CSS Views/Availability.css" rel="stylesheet" />
}

<div class="container col-sm-4 col-md-7 col-lg-4 mt-5">
    <div class="card col-sm-2 col-md-4 col-lg-2">
        <h3 class="card-header" id="monthAndYear"></h3>
        <table class="table table-bordered table-responsive-sm" id="calendar">
            <thead>
                <tr>
                    <th>Zo</th>
                    <th>Ma</th>
                    <th>Di</th>
                    <th>Wo</th>
                    <th>Do</th>
                    <th>Vr</th>
                    <th>Za</th>
                </tr>
            </thead>

            <tbody id="calendar-body">
            </tbody>
        </table>

        <div class="form-inline">

            <button class="btn btn-outline-primary col-sm-6" id="previous" onclick="previous()">Previous</button>

            <button class="btn btn-outline-primary col-sm-6" id="next" onclick="next()">Next</button>
        </div>
        <br />
        <form class="form-inline">
            <select class="form-control col-md-4" name="month" id="month" onchange="jump()">
                <option value=0>Jan</option>
                <option value=1>Feb</option>
                <option value=2>Mrt</option>
                <option value=3>Apr</option>
                <option value=4>Mei</option>
                <option value=5>Jun</option>
                <option value=6>Jul</option>
                <option value=7>Aug</option>
                <option value=8>Sep</option>
                <option value=9>Okt</option>
                <option value=10>Nov</option>
                <option value=11>Dec</option>
            </select>


            <label for="year"></label><select class="form-control col-md-4" name="year" id="year" onchange="jump()">
                <option value=2019>2019</option>
                <option value=2020>2020</option>
            </select>
        </form>
    </div>
</div>



